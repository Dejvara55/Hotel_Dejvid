@model IEnumerable<Hotel_Dejvid.Models.Rezervacija>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GostId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SobaId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DatumDolaska)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DatumOdlaska)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GostId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SobaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatumDolaska)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatumOdlaska)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RezervacijaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RezervacijaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RezervacijaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
